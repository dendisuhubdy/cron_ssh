minutes:
    numberSpec
    {
        d_cronData.setMinutes();
    }
;

hours:
    numberSpec
    {
        d_cronData.setHours();
    }
;

dayOfMonth:
    numberSpec
    {
        d_cronData.setDayOfMonth();
    }
;

monthOfYear:
    timeSpec
    {
        d_cronData.setMonthOfYear();
    }
;

dayOfWeek:
    timeSpec
    {
        d_cronData.setDayOfWeek();
        d_scanner.getTail();
    }
;

command:
    noWSsequence
    {
        d_cronData.setCommand($1);
    }
;

cronLine:
    minutes hours dayOfMonth monthOfYear dayOfWeek command
    {
        d_cronData.process();
    }
;
