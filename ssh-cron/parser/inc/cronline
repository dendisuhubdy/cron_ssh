_minutes:
    time_numberedSpec
    ws_separator
    {
        d_cronData.setMinutes();
    }
;

_hours:
    time_numberedSpec
    ws_separator
    {
        d_cronData.setHours();
    }
;

_dayOfMonth:
    time_numberedSpec
    ws_separator
    {
        d_cronData.setDayOfMonth();
    }
;

_monthOfYear:
    time_spec
    ws_separator
    {
        d_cronData.setMonthOfYear();
    }
;

_dayOfWeek:
    time_spec
    ws_separator
    {
        d_cronData.setDayOfWeek();
    }
;

_command:
    token_noWs
    opt_tokens
    {
        d_cronData.setCommand($1 + $2);
    }
;

cronLine:
    _minutes _hours _dayOfMonth _monthOfYear _dayOfWeek _command
    {
        d_cronData.process();
    }
;
